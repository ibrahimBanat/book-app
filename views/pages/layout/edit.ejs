<form action="/updateBook/<%= bookData.id %>?_method=put" method="post">
  <label for="image_url">Image URL</label>
  <input
    type="text"
    name="image_url"
    id="image_url"
    value="<%= bookData.image_url %>"
  />
  <label for="title">Title</label>
  <input id="title" type="text" name="title" value="<%= bookData.title %>" />
  <label for="author">Author</label>
  <input id="author" type="text" name="author" value="<%= bookData.author %>" />
  <label for="description">description</label>
  <textarea id="description" type="text" name="description">
<%=
  bookData.description %>"</textarea
  >
  <button id="save">Save</button>
</form>
