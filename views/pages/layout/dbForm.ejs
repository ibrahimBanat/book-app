<form action="/books" method="POST">
  <input type="hidden" name="author" value="<%= elementD.author %>" reqiuerd />
  <input type="hidden" name="title" value="<%= elementD.title %>" reqiuerd />
  <input type="hidden" name="isbn" value="<%= elementD.isbn %>" reqiuerd />
  <input
    type="hidden"
    name="image_url"
    value="<%= elementD.image_url %>"
    reqiuerd
  />
  <input
    type="hidden"
    name="description"
    value="<%= elementD.desc %>"
    reqiuerd
  />
  <button type="submit">Add to the Shelf</button>
</form>
